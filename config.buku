#!/usr/bin/env bash

_rofi () {
    rofi -dmenu -i -no-levenshtein-sort -width 1000 "$@"
}

# keybindings
switch_view="Alt+Tab"
new_bookmark="Alt+n"
actions="Alt+a"
edit="Alt+e"
delete="Alt+d"
copyurl="Alt+l"

#libnotify messages
notifysend='true'
#display oldest bookmarks on top by default
reverselist='false'

# colors
help_color="#2d7ed8"
